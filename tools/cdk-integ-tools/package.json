  },
  "dependencies": {
    "@aws-cdk/cloudformation-diff": "0.0.0",
    "@aws-cdk/cx-api": "0.0.0",
    "@aws-cdk/assert": "0.0.0",
    "aws-cdk": "0.0.0",
    "fs-extra": "^9.0.1",
    "yargs": "^15.4.1"
  },
  "keywords": [
    "aws",
