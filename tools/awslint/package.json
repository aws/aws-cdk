{
  "name": "awslint",
  "private": true,
  "version": "1.12.0",
  "description": "Enforces the AWS Construct Library guidelines",
  "main": "index.js",
  "scripts": {
    "build": "tsc -b && chmod +x bin/awslint",
    "lint": "eslint . --ext .ts,.js --ignore-path .gitignore  && pkglint",
    "test": "echo ok",
    "watch": "tsc -b -w",
    "build+test+package": "npm run build+test",
    "build+test": "npm run build && npm test"
  },
  "bin": {
    "awslint": "bin/awslint"
  },
  "dependencies": {
    "camelcase": "^5.3.1",
    "colors": "^1.4.0",
    "fs-extra": "^8.1.0",
    "jsii-reflect": "^0.19.0",
    "jsii-spec": "^0.19.0",
    "yargs": "^14.2.0"
  },
  "devDependencies": {
    "@types/colors": "^1.2.1",
    "@types/fs-extra": "^8.0.0",
    "@types/yargs": "^13.0.3",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "eslint": "^6.5.1",
    "eslint-plugin-import": "^2.18.2",
    "typescript": "~3.6.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/aws-cdk.git"
  },
  "homepage": "https://github.com/aws/aws-cdk",
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com",
    "organization": true
  },
  "keywords": [
    "aws",
    "cdk"
  ],
  "engines": {
    "node": ">= 8.10.0"
  }
}
