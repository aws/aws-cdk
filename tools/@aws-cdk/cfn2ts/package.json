{
  "name": "@aws-cdk/cfn2ts",
  "private": true,
  "version": "0.0.0",
  "description": "Generates typescript types from CloudFormation spec, with support for enrichments",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/aws-cdk.git",
    "directory": "tools/@aws-cdk/cfn2ts"
  },
  "bin": {
    "cfn2ts": "bin/cfn2ts"
  },
  "scripts": {
    "build": "cdk-build",
    "watch": "cdk-watch",
    "lint": "cdk-lint",
    "test": "cdk-test",
    "pkglint": "pkglint -f",
    "build+test+package": "yarn build+test",
    "build+test": "yarn build && yarn test",
    "build+extract": "yarn build",
    "build+test+extract": "yarn build+test"
  },
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com",
    "organization": true
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-cdk/cfnspec": "0.0.0",
    "codemaker": "^1.68.0",
    "fast-json-patch": "^3.1.1",
    "fs-extra": "^9.1.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@aws-cdk/cdk-build-tools": "0.0.0",
    "@aws-cdk/pkglint": "0.0.0",
    "@types/fs-extra": "^8.1.2",
    "@types/jest": "^27.5.2",
    "@types/yargs": "^15.0.14",
    "jest": "^27.5.1"
  },
  "keywords": [
    "aws",
    "cdk"
  ],
  "homepage": "https://github.com/aws/aws-cdk",
  "engines": {
    "node": ">= 14.15.0"
  },
  "ubergen": {
    "exclude": true
  }
