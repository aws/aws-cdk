{
  "name": "@aws-cdk/individual-pkg-gen",
  "version": "0.0.0",
  "private": true,
  "description": "A tool for copying V1 packages to be released separately in V2",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/aws-cdk.git",
    "directory": "tools/@aws-cdk/individual-pkg-gen"
  },
  "scripts": {
    "build": "cdk-build",
    "watch": "cdk-watch",
    "lint": "cdk-lint",
    "test": "cdk-test",
    "pkglint": "pkglint -f",
    "build+test+package": "yarn build+test",
    "build+test": "yarn build && yarn test",
    "build+extract": "yarn build",
    "build+test+extract": "yarn build+test"
  },
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com",
    "organization": true
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@aws-cdk/cdk-build-tools": "0.0.0",
    "@aws-cdk/pkglint": "0.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.5.2"
  },
  "dependencies": {
    "aws-cdk-migration": "0.0.0",
    "fs-extra": "^9.1.0",
    "@lerna/project": "4.0.0"
  },
  "keywords": [
    "aws",
    "cdk"
  ],
  "homepage": "https://github.com/aws/aws-cdk",
  "engines": {
    "node": ">= 14.15.0"
  },
  "ubergen": {
    "exclude": true
  }
}
