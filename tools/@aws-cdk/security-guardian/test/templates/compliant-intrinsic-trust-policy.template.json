{
	"Resources": {
		"CompliantRole": {
			"Type": "AWS::IAM::Role",
			"Properties": {
				"AssumeRolePolicyDocument": {
					"Version": "2012-10-17",
					"Statement": [
						{
							"Effect": "Allow",
							"Principal": {
								"AWS": [
									{
										"Fn::GetAtt": [
											"OtherRole",
											"Arn"
										]
									},
									{
										"Fn::Sub": "arn:aws:iam::${AWS::AccountId}:root"
									}
								]
							},
							"Action": "sts:AssumeRole"
						}
					]
				}
			}
		},
		"OtherRole": {
			"Type": "AWS::IAM::Role",
			"Properties": {
				"AssumeRolePolicyDocument": {
					"Version": "2012-10-17",
					"Statement": [
						{
							"Effect": "Allow",
							"Principal": {
								"Service": "lambda.amazonaws.com"
							},
							"Action": "sts:AssumeRole"
						}
					]
				}
			}
		}
	}
}