{
  "name": "pkglint",
  "version": "0.26.0",
  "private": true,
  "description": "Validate and fix package.json files",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/awslabs/aws-cdk"
  },
  "pkglint": {
    "ignore": true
  },
  "bin": {
    "pkglint": "bin/pkglint"
  },
  "scripts": {
    "build": "tsc && tslint -p . && chmod +x bin/pkglint",
    "watch": "tsc -w",
    "lint": "tsc && tslint -p . --force"
  },
  "keywords": [
    "aws",
    "cdk",
    "constructs",
    "s3"
  ],
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/colors": "^1.2.1",
    "@types/fs-extra": "^4.0.8",
    "@types/semver": "^5.5.0",
    "@types/yargs": "^8.0.3"
  },
  "dependencies": {
    "case": "^1.5.5",
    "colors": "^1.3.2",
    "fs-extra": "^7.0.0",
    "semver": "^5.6.0",
    "yargs": "^9.0.1"
  }
}
