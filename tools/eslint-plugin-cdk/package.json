  },
  "devDependencies": {
    "@types/eslint": "^7.2.8",
    "@types/fs-extra": "^8.1.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^10.17.56",
    "eslint-plugin-rulesdir": "^0.2.0",
    "jest": "^26.6.3",
    "typescript": "~3.9.9"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.23.0",
    "fs-extra": "^9.1.0"
  },
  "jest": {
    "testMatch": [
      "**/*.test.js"
    ]
  },
  "keywords": [],
