  "keywords": [
    "aws",
    "cdk",
    "constructs",
    "dynamodb",
    "global"
  ],
  "dependencies": {
    "@aws-cdk/aws-cloudformation": "^1.0.0",
    "@aws-cdk/aws-dynamodb": "^1.0.0",
    "@aws-cdk/aws-iam": "^1.0.0",
    "@aws-cdk/aws-lambda": "^1.0.0",
    "@aws-cdk/core": "^1.0.0"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.0.0",
    "cdk-build-tools": "^1.0.0",
    "cdk-integ-tools": "^1.0.0",
    "cfn2ts": "^1.0.0",
    "pkglint": "^1.0.0"
  },
  "peerDependencies": {
    "@aws-cdk/aws-cloudformation": "^1.0.0",
    "@aws-cdk/aws-dynamodb": "^1.0.0",
    "@aws-cdk/aws-iam": "^1.0.0",
    "@aws-cdk/aws-lambda": "^1.0.0",
    "@aws-cdk/core": "^1.0.0"
  },
  "scripts": {
    "build": "cdk-build",
    "watch": "cdk-watch",
    "lint": "cdk-lint",
    "test": "cdk-test",
    "integ": "cdk-integ",
    "pkglint": "pkglint -f",
    "package": "cdk-package",
    "awslint": "cdk-awslint",
    "cfn2ts": "cfn2ts",
    "build+test+package": "npm run build+test && npm run package",
    "build+test": "npm run build && npm test"
  },
  "engines": {
    "node": ">= 8.10.0"
