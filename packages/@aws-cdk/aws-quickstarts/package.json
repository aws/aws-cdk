{
  "name": "@aws-cdk/aws-quickstarts",
  "version": "0.8.0",
  "description": "AWS Quickstarts for the CDK",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "com.amazonaws.cdk.aws.quickstarts",
        "maven": {
          "groupId": "com.amazonaws.cdk",
          "artifactId": "aws-quickstarts"
        }
      },
      "dotnet": {
        "namespace": "Amazon.CDK.AWS.QuickStarts"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/awslabs/aws-cdk"
  },
  "scripts": {
    "build": "cdk-build",
    "watch": "cdk-watch",
    "lint": "cdk-lint",
    "pkglint": "pkglint -f",
    "test": "cdk-test",
    "package": "cdk-package"
  },
  "keywords": [
    "aws",
    "cdk",
    "constructs",
    "quickstarts"
  ],
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "cdk-build-tools": "^0.8.0",
    "pkglint": "^0.8.0"
  },
  "dependencies": {
    "@aws-cdk/aws-cloudformation": "^0.8.0",
    "@aws-cdk/aws-ec2": "^0.8.0",
    "@aws-cdk/aws-iam": "^0.8.0",
    "@aws-cdk/aws-rds": "^0.8.0",
    "@aws-cdk/cdk": "^0.8.0",
    "@aws-cdk/util": "^0.8.0"
  },
  "bundledDependencies": [
    "@aws-cdk/util"
  ]
}
