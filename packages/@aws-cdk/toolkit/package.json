    "json-diff": "^0.10.0",
    "minimatch": "^9.0.5",
    "p-limit": "^3.1.0",
    "promptly": "^3.2.0",
    "proxy-agent": "^6.4.0",
    "semver": "^7.6.3",
    "split2": "^4.2.0",
    "strip-ansi": "^6.0.1",
    "table": "^6.8.2",
    "uuid": "^8.3.2",
