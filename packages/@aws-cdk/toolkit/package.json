    "yaml": "1.10.2",
    "yargs": "^16.2.0"
  },
  "repository": {
    "url": "https://github.com/aws/aws-cdk.git",
    "type": "git",
    "directory": "packages/@aws-cdk/toolkit"
  },
  "keywords": [
    "aws",
    "cdk"
  ],
  "homepage": "https://github.com/aws/aws-cdk",
  "engines": {
    "node": ">= 14.15.0"
  },
  "awscdkio": {
    "announce": false
  },
  "stability": "experimental",
  "maturity": "experimental",
  "publishConfig": {
    "access": "restricted",
    "tag": "latest"
  }
