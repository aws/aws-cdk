{
 "Resources": {
  "VectorBucket7AA37AC5": {
   "Type": "AWS::S3Vectors::VectorBucket",
   "Properties": {
    "VectorBucketName": "integ-vb-index-kms"
   },
   "UpdateReplacePolicy": "Delete",
   "DeletionPolicy": "Delete"
  },
  "IndexKeyA7F7F7D2": {
   "Type": "AWS::KMS::Key",
   "Properties": {
    "Description": "Created by IndexWithKMSEncryptionTest/Index",
    "EnableKeyRotation": true,
    "KeyPolicy": {
     "Statement": [
      {
       "Action": "kms:*",
       "Effect": "Allow",
       "Principal": {
        "AWS": {
         "Fn::Join": [
          "",
          [
           "arn:",
           {
            "Ref": "AWS::Partition"
           },
           ":iam::",
           {
            "Ref": "AWS::AccountId"
           },
           ":root"
          ]
         ]
        }
       },
       "Resource": "*"
      },
      {
       "Action": "kms:Decrypt",
       "Condition": {
        "ArnLike": {
         "aws:SourceArn": {
          "Fn::Join": [
           "",
           [
            "arn:",
            {
             "Ref": "AWS::Partition"
            },
            ":s3vectors:",
            {
             "Ref": "AWS::Region"
            },
            ":",
            {
             "Ref": "AWS::AccountId"
            },
            ":bucket/*"
           ]
          ]
         }
        },
        "StringEquals": {
         "aws:SourceAccount": {
          "Ref": "AWS::AccountId"
         }
        },
        "ForAnyValue:StringEquals": {
         "kms:EncryptionContextKeys": [
          "aws:s3vectors:arn",
          "aws:s3vectors:resource-id"
         ]
        }
       },
       "Effect": "Allow",
       "Principal": {
        "Service": "indexing.s3vectors.amazonaws.com"
       },
       "Resource": "*",
       "Sid": "AllowS3VectorsIndexingAccess"
      }
     ],
     "Version": "2012-10-17"
    }
   },
   "UpdateReplacePolicy": "Retain",
   "DeletionPolicy": "Retain"
  },
  "Index4A79713B": {
   "Type": "AWS::S3Vectors::Index",
   "Properties": {
