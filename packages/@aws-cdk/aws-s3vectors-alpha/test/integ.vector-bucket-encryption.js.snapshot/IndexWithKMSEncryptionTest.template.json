    "EncryptionConfiguration": {
     "KmsKeyArn": {
      "Fn::GetAtt": [
       "IndexKeyA7F7F7D2",
       "Arn"
      ]
     },
     "SseType": "aws:kms"
    },
    "IndexName": "integ-index-kms",
    "VectorBucketArn": {
     "Fn::GetAtt": [
