{
  "name": "@aws-cdk/kms",
  "version": "0.7.3-beta",
  "description": "CDK Constructs for AWS KMS",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "names": {
      "java": "com.amazonaws.cdk.kms",
      "dotnet": "Aws.Cdk.Kms"
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/awslabs/aws-cdk"
  },
  "scripts": {
    "build": "cfn2ts --scope=AWS::KMS && jsii && tslint -p . && pkglint",
    "watch": "jsii -w",
    "lint": "tsc && tslint -p .",
    "test": "nodeunit test/test.*.js",
    "pkglint": "pkglint -f"
  },
  "keywords": [
    "aws",
    "cdk",
    "constructs",
    "kms"
  ],
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com"
  },
  "license": "LicenseRef-LICENSE",
  "devDependencies": {
    "@aws-cdk/assert": "^0.7.3-beta",
    "cfn2ts": "^0.7.3-beta",
    "pkglint": "^0.7.3-beta"
  },
  "dependencies": {
    "@aws-cdk/core": "^0.7.3-beta",
    "@aws-cdk/iam": "^0.7.3-beta"
  }
}
