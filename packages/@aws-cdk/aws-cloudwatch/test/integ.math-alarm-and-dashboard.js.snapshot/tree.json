          "Alarm": {
            "id": "Alarm",
            "path": "aws-cdk-cloudwatch/Alarm",
            "children": {
              "Resource": {
                "id": "Resource",
                "path": "aws-cdk-cloudwatch/Alarm/Resource",
                "attributes": {
                  "aws:cdk:cloudformation:type": "AWS::CloudWatch::Alarm",
                  "aws:cdk:cloudformation:props": {
                    "comparisonOperator": "GreaterThanOrEqualToThreshold",
                    "evaluationPeriods": 3,
                    "metrics": [
                      {
                        "expression": "m1+m2",
                        "id": "expr_1",
                        "label": "Total Messages"
                      },
                      {
                        "metricStat": {
                          "metric": {
                            "metricName": "ApproximateNumberOfMessagesVisible",
                            "namespace": "AWS/SQS",
                            "dimensions": [
                              {
                                "name": "QueueName",
                                "value": {
                                  "Fn::GetAtt": [
                                    "queue",
                                    "QueueName"
                                  ]
                                }
                              }
                            ]
                          },
                          "period": 60,
                          "stat": "Average"
                        },
                        "id": "m1",
                        "label": "Visible Messages",
                        "returnData": false
                      },
                      {
                        "metricStat": {
                          "metric": {
                            "metricName": "ApproximateNumberOfMessagesNotVisible",
                            "namespace": "AWS/SQS",
                            "dimensions": [
                              {
                                "name": "QueueName",
                                "value": {
                                  "Fn::GetAtt": [
                                    "queue",
                                    "QueueName"
                                  ]
                                }
                              }
                            ]
                          },
                          "period": 60,
                          "stat": "Average"
                        },
                        "id": "m2",
                        "label": "NotVisible Messages",
                        "returnData": false
                      }
                    ],
                    "threshold": 100
                  }
                },
                "constructInfo": {
                  "fqn": "@aws-cdk/aws-cloudwatch.CfnAlarm",
                  "version": "0.0.0"
                }
              }
            },
            "constructInfo": {
              "fqn": "@aws-cdk/aws-cloudwatch.Alarm",
              "version": "0.0.0"
