{
  "name": "@aws-cdk/example-construct-library",
  "private": true,
  "version": "0.0.0",
  "description": "An example CDK Construct Library that can serve as a template for creating new libraries",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "software.amazon.awscdk.example.construct.library",
        "maven": {
          "groupId": "software.amazon.awscdk",
          "artifactId": "cdk-example-construct-library"
        }
      },
      "dotnet": {
        "namespace": "Amazon.CDK.Example.Construct.Library",
        "packageId": "Amazon.CDK.Example.Construct.Library",
        "iconUrl": "https://raw.githubusercontent.com/aws/aws-cdk/main/logo/default-256-dark.png"
      },
      "python": {
        "distName": "aws-cdk.example-construct-library",
        "module": "aws_cdk.example_construct_library",
        "classifiers": [
          "Framework :: AWS CDK",
          "Framework :: AWS CDK :: 2"
        ]
      }
    },
    "metadata": {
      "jsii": {
        "rosetta": {
          "strict": true
        }
      }
    },
    "tsconfig": "tsconfig.json",
    "validateTsconfig": "minimal"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/aws-cdk.git",
    "directory": "packages/@aws-cdk/example-construct-library"
  },
  "scripts": {
    "build": "cdk-build",
    "watch": "cdk-watch",
    "lint": "cdk-lint",
    "test": "cdk-test",
    "integ": "integ-runner --unstable=toolkit-lib-engine --language javascript",
    "pkglint": "pkglint -f",
    "package": "cdk-package",
    "awslint": "cdk-awslint",
    "build+test": "yarn build && yarn test",
    "build+test+package": "yarn build+test && yarn package",
    "compat": "cdk-compat",
    "rosetta:extract": "yarn --silent jsii-rosetta extract",
    "build+extract": "yarn build && yarn rosetta:extract",
    "build+test+extract": "yarn build+test && yarn rosetta:extract"
  },
  "keywords": [
    "aws",
    "cdk",
    "example",
    "construct",
    "library"
  ],
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com",
    "organization": true
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "aws-cdk-lib": "0.0.0",
    "@aws-cdk/cdk-build-tools": "0.0.0",
    "@aws-cdk/integ-runner": "^2.195.0",
    "@aws-cdk/pkglint": "0.0.0",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0"
  },
  "homepage": "https://github.com/aws/aws-cdk",
  "peerDependencies": {
    "aws-cdk-lib": "^0.0.0",
    "constructs": "^10.0.0"
  },
  "separate-module": false,
  "engines": {
    "node": ">= 18.0.0"
  },
  "stability": "experimental",
  "maturity": "experimental",
  "awscdkio": {
    "announce": false
  },
  "cdk-build": {
    "env": {
      "AWSLINT_BASE_CONSTRUCT": true
    }
  },
  "awslint": {
    "exclude": [
      "prefer-ref-interface:@aws-cdk/example-construct-library.ExampleResourceProps.bucket",
      "prefer-ref-interface:aws-cdk-lib.aws_s3.GrantReplicationPermissionDestinationProps.bucket",
      "prefer-ref-interface:aws-cdk-lib.aws_s3.GrantReplicationPermissionDestinationProps.encryptionKey",
      "prefer-ref-interface:aws-cdk-lib.aws_s3.GrantReplicationPermissionProps.sourceDecryptionKey",
      "prefer-ref-interface:@aws-cdk/example-construct-library.ExampleResourceProps.role",
      "prefer-ref-interface:@aws-cdk/example-construct-library.ExampleResourceProps.securityGroups",
      "prefer-ref-interface:@aws-cdk/example-construct-library.ExampleResourceProps.vpc",
      "prefer-ref-interface:aws-cdk-lib.aws_ec2.ClientVpnEndpointOptions.securityGroups",
      "prefer-ref-interface:aws-cdk-lib.aws_ec2.SubnetSelection.subnets",
      "prefer-ref-interface:aws-cdk-lib.aws_ec2.ISubnet.associateNetworkAcl.acl",
      "prefer-ref-interface:aws-cdk-lib.aws_ec2.InterfaceVpcEndpointOptions.securityGroups"
    ]
  }
}
