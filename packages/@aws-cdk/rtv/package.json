{
  "name": "@aws-cdk/rtv",
  "version": "0.7.2-beta",
  "description": "Runtime values support for the AWS CDK",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "names": {
      "java": "com.amazonaws.cdk.rtv",
      "dotnet": "Aws.Cdk.Rtv"
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/awslabs/aws-cdk"
  },
  "scripts": {
    "prepare": "jsii && tslint -p . && pkglint",
    "watch": "jsii -w",
