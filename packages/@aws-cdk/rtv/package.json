    "test": "nyc nodeunit test/test.*.js",
    "pkglint": "pkglint -f"
  },
  "keywords": [
    "aws",
    "cdk",
    "rtv"
  ],
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com"
  },
  "license": "LicenseRef-LICENSE",
  "devDependencies": {
    "pkglint": "^0.7.1"
  },
  "dependencies": {
    "@aws-cdk/core": "^0.7.2-beta",
    "@aws-cdk/iam": "^0.7.2-beta",
    "@aws-cdk/resources": "^0.7.2-beta"
  }
}
