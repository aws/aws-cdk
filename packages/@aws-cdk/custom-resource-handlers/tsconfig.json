{
    "compilerOptions": {
        "declarationMap": false,
        "inlineSourceMap": true,
        "inlineSources": true,
        "alwaysStrict": true,
        "declaration": true,
        "experimentalDecorators": true,
        "incremental": true,
        "lib": [
        "es2020"
        ],
        "module": "CommonJS",
        "noEmitOnError": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "strict": true,
        "strictNullChecks": true,
        "strictPropertyInitialization": true,
        "stripInternal": false,
        "target": "ES2020",
        "composite": false,
        "tsBuildInfoFile": "tsconfig.tsbuildinfo"
    },
    "include": [
        "**/*.ts",
        "**/*.d.ts",
    ],
    "exclude": [
        "**/test/**/*.ts",
    ],
}
