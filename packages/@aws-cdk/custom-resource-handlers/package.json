    "aws-sdk-client-mock": "^3.0.0",
    "aws-sdk-client-mock-jest": "^3.0.0",
    "aws-sdk-mock": "5.8.0",
    "jest": "^29.7.0",
    "sinon": "^9.2.4",
    "nock": "^13.3.8",
    "fs-extra": "^11.1.1",
    "esbuild": "^0.19.5"
