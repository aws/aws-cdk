    "cfn2ts": "0.0.0",
    "jest": "^26.6.1",
    "pkglint": "0.0.0",
    "sinon": "^9.2.1",
    "ts-jest": "^26.4.3"
  },
  "dependencies": {
    "@aws-cdk/aws-applicationautoscaling": "0.0.0",
