    "@aws-cdk/aws-cloudfront": "0.0.0",
    "@aws-cdk/aws-ec2": "0.0.0",
    "@aws-cdk/aws-iam": "0.0.0",
    "@aws-cdk/aws-lambda": "0.0.0",
    "@aws-cdk/aws-s3": "0.0.0",
    "@aws-cdk/aws-s3-assets": "0.0.0",
    "@aws-cdk/lambda-layer-awscli": "0.0.0",
    "@aws-cdk/core": "0.0.0",
    "constructs": "^10.0.0"
  },
  "engines": {
    "node": ">= 10.13.0 <13 || >=13.7.0"
  },
  "stability": "stable",
  "maturity": "stable",
  "awscdkio": {
    "announce": false
