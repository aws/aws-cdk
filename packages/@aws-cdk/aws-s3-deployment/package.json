{
  "name": "@aws-cdk/aws-s3-deployment",
  "version": "0.31.0",
  "description": "Constructs for deploying contents to S3 buckets",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "software.amazon.awscdk.services.s3.deployment",
        "maven": {
          "groupId": "software.amazon.awscdk",
          "artifactId": "s3-deployment"
        }
      },
      "dotnet": {
        "namespace": "Amazon.CDK.AWS.S3.Deployment",
        "packageId": "Amazon.CDK.AWS.S3.Deployment",
        "signAssembly": true,
        "assemblyOriginatorKeyFile": "../../key.snk"
      },
      "python": {
        "distName": "aws-cdk.aws-s3-deployment",
        "module": "aws_cdk.aws_s3_deployment"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/awslabs/aws-cdk.git",
    "directory": "packages/@aws-cdk/aws-s3-deployment"
  },
  "scripts": {
    "build": "cdk-build",
    "watch": "cdk-watch",
    "lint": "cdk-lint",
    "test": "cdk-test",
    "integ": "cdk-integ",
    "pkglint": "pkglint -f",
    "package": "cdk-package",
    "awslint": "cdk-awslint",
    "build:test:package": "npm run build:test && npm run package",
    "build:test": "npm run build && npm test"
  },
  "cdk-build": {
    "pre": [
      "/bin/bash lambda/build.sh"
    ],
    "test": [
      "/bin/bash lambda/test.sh"
    ]
  },
  "keywords": [
    "aws",
    "cdk",
    "constructs",
    "s3"
  ],
  "nyc": {
    "exclude": [
      "coverage/**",
      "test/**",
      "examples/**",
      "lib/*.generated.js",
      "build-tools/**",
      "lambda/**"
    ]
  },
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com",
    "organization": true
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@aws-cdk/assert": "^0.31.0",
    "cdk-build-tools": "^0.31.0",
    "cdk-integ-tools": "^0.31.0",
    "pkglint": "^0.31.0"
  },
  "dependencies": {
    "@aws-cdk/assets": "^0.31.0",
    "@aws-cdk/aws-cloudformation": "^0.31.0",
    "@aws-cdk/aws-iam": "^0.31.0",
    "@aws-cdk/aws-lambda": "^0.31.0",
    "@aws-cdk/aws-s3": "^0.31.0",
    "@aws-cdk/cdk": "^0.31.0"
  },
  "homepage": "https://github.com/awslabs/aws-cdk",
  "peerDependencies": {
    "@aws-cdk/assets": "^0.31.0",
    "@aws-cdk/aws-cloudformation": "^0.31.0",
    "@aws-cdk/aws-iam": "^0.31.0",
    "@aws-cdk/aws-lambda": "^0.31.0",
    "@aws-cdk/aws-s3": "^0.31.0",
    "@aws-cdk/cdk": "^0.31.0"
  },
  "engines": {
    "node": ">= 8.10.0"
  }
}
