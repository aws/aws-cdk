{
  "name": "@aws-cdk/cfnspec",
  "description": "The CloudFormation resource specification used by @aws-cdk packages",
  "version": "0.8.1",
  "scripts": {
    "update": "/bin/bash build-tools/update.sh",
    "build": "tsc && tslint -p . && node build-tools/build",
    "test": "nyc nodeunit test/test.*.js",
    "watch": "tsc -w",
    "lint": "tslint -p ."
  },
  "nyc": {
    "lines": 50
  },
  "pkglint": {
    "ignore": true
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "devDependencies": {
    "@types/fs-extra": "^4.0.8",
    "@types/md5": "^2.1.32",
    "cdk-build-tools": "^0.8.1",
    "fast-json-patch": "^2.0.6",
    "fs-extra": "^4.0.2",
    "json-diff": "^0.3.1",
    "sort-json": "^2.0.0"
  },
  "dependencies": {
    "md5": "^2.2.1"
  },
  "repository": {
    "url": "git://github.com/awslabs/aws-cdk",
    "type": "git"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com"
  },
  "keywords": [
    "aws",
    "cdk"
  ]
}
