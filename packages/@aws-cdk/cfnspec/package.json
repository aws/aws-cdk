{
  "name": "@aws-cdk/cfnspec",
  "description": "The CloudFormation resource specification used by @aws-cdk packages",
  "version": "0.10.0",
  "scripts": {
    "update": "cdk-build && /bin/bash build-tools/update.sh",
    "build": "cdk-build && node build-tools/build",
    "watch": "cdk-watch",
    "pkglint": "pkglint -f",
    "test": "cdk-test",
    "package": "cdk-package"
  },
  "nyc": {
    "lines": 50
  },
  "pkglint": {
    "ignore": true
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "devDependencies": {
    "@types/fs-extra": "^4.0.8",
    "@types/md5": "^2.1.32",
    "cdk-build-tools": "^0.10.0",
    "fast-json-patch": "^2.0.6",
    "fs-extra": "^4.0.2",
    "json-diff": "^0.3.1",
    "pkglint": "^0.10.0",
    "sort-json": "^2.0.0"
  },
  "dependencies": {
    "md5": "^2.2.1"
  },
  "repository": {
    "url": "https://github.com/awslabs/aws-cdk.git",
