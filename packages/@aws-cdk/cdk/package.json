{
  "name": "@aws-cdk/cdk",
  "version": "0.31.0",
  "description": "AWS Cloud Development Kit Core Library",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "software.amazon.awscdk",
        "maven": {
          "groupId": "software.amazon.awscdk",
          "artifactId": "cdk"
        }
      },
      "dotnet": {
        "namespace": "Amazon.CDK",
        "packageId": "Amazon.CDK",
        "signAssembly": true,
        "assemblyOriginatorKeyFile": "../../key.snk"
      },
      "python": {
        "distName": "aws-cdk.cdk",
        "module": "aws_cdk.cdk"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/awslabs/aws-cdk.git",
    "directory": "packages/@aws-cdk/cdk"
  },
  "awslint": {
    "exclude": [
      "construct-ctor:@aws-cdk/cdk.App.<initializer>",
      "construct-ctor:@aws-cdk/cdk.Root.<initializer>",
      "construct-ctor:@aws-cdk/cdk.Stack.<initializer>.params*"
    ]
  },
  "scripts": {
    "build": "cdk-build",
    "watch": "cdk-watch",
    "lint": "cdk-lint",
    "test": "cdk-test",
    "pkglint": "pkglint -f",
    "package": "cdk-package",
    "awslint": "cdk-awslint",
    "build+test+package": "npm run build+test && npm run package",
    "build+test": "npm run build && npm test"
  },
  "nyc": {
    "statements": 55,
    "lines": 55,
    "branches": 35
  },
  "keywords": [
    "aws",
    "cdk",
    "jsii",
    "cdk-core"
  ],
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com",
    "organization": true
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/lodash": "^4.14.130",
    "cdk-build-tools": "^0.31.0",
    "cfn2ts": "^0.31.0",
    "fast-check": "^1.14.0",
    "lodash": "^4.17.11",
    "pkglint": "^0.31.0"
  },
  "dependencies": {
    "@aws-cdk/cx-api": "^0.31.0"
  },
  "homepage": "https://github.com/awslabs/aws-cdk",
  "peerDependencies": {
    "@aws-cdk/cx-api": "^0.31.0"
  },
  "engines": {
    "node": ">= 8.10.0"
  }
}
