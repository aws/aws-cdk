{
  "name": "@aws-cdk/core",
  "version": "0.7.0",
  "description": "AWS Cloud Development Kit for Javascript",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "bundledDependencies": [
      "json-diff"
    ],
    "names": {
      "java": "com.amazonaws.cdk",
      "dotnet": "Aws.Cdk"
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/awslabs/aws-cdk"
  },
  "scripts": {
    "prepare": "jsii && tslint -p . && pkglint",
    "watch": "jsii -w",
    "lint": "tsc && tslint -p . --force",
    "test": "nyc --reporter html --reporter text nodeunit --output junit test/**/test.*.js test/test.*.js",
    "pkglint": "pkglint -f"
  },
  "keywords": [
    "aws",
    "cdk",
    "jsii",
    "cdk-core"
  ],
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "cfn2ts": "^0.7.0",
    "pkglint": "^0.7.0"
  },
  "dependencies": {
    "@aws-cdk/cx-api": "^0.7.0",
    "json-diff": "^0.3.1"
  }
}
