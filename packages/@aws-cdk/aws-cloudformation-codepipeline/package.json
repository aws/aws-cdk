  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@aws-cdk/assert": "^0.8.1",
    "@aws-cdk/aws-codebuild": "^0.8.1",
    "@aws-cdk/aws-codebuild-codepipeline": "^0.8.1",
    "@aws-cdk/aws-codecommit": "^0.8.1",
    "@aws-cdk/aws-codecommit-codepipeline": "^0.8.1",
    "@aws-cdk/aws-s3": "^0.8.1",
    "cdk-build-tools": "^0.8.1",
    "cdk-integ-tools": "^0.8.1",
    "pkglint": "^0.8.1"
  },
  "dependencies": {
    "@aws-cdk/aws-codepipeline": "^0.8.1",
    "@aws-cdk/aws-iam": "^0.8.1",
    "@aws-cdk/cdk": "^0.8.1"
  },
  "homepage": "https://github.com/awslabs/aws-cdk"
