  },
  "homepage": "https://github.com/aws/aws-cdk",
  "peerDependencies": {
    "@aws-cdk/aws-iam": "0.0.0",
    "@aws-cdk/aws-kms": "0.0.0",
    "@aws-cdk/core": "0.0.0",
    "@aws-cdk/cx-api": "0.0.0",
    "constructs": "^2.0.0"
  },
  "engines": {
    "node": ">= 10.12.0"
