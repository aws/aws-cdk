{
  "name": "@aws-cdk/custom-resources",
  "version": "0.7.0",
  "description": "CDK Constructs to make it easier to build custom resources",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "bundledDependencies": [],
    "names": {
      "java": "com.amazonaws.cdk.customresources",
      "dotnet": "Aws.Cdk.CustomResources"
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/awslabs/aws-cdk"
  },
  "scripts": {
    "prepare": "jsii && tslint -p . && pkglint",
    "watch": "jsii -w",
    "lint": "tsc && tslint -p . --force",
    "test": "nodeunit test/test.*.js && cdk-integ-assert",
    "integ": "cdk-integ",
    "pkglint": "pkglint -f"
  },
  "keywords": [
    "aws",
    "cdk",
    "constructs",
    "custom-resources"
  ],
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@aws-cdk/assert": "^0.7.0",
    "aws-cdk": "^0.7.0",
    "pkglint": "^0.7.0"
  },
  "dependencies": {
    "@aws-cdk/core": "^0.7.0",
    "@aws-cdk/iam": "^0.7.0",
    "@aws-cdk/lambda": "^0.7.0",
    "@aws-cdk/resources": "^0.7.0",
    "@aws-cdk/sns": "^0.7.0"
  }
}
