{
  "name": "@aws-cdk/quickstarts",
  "version": "0.7.3-beta",
  "description": "AWS Quickstarts for the CDK",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "com.amazonaws.cdk.quickstarts",
        "maven": {
          "groupId": "com.amazonaws.cdk",
          "artifactId": "cdk-quickstarts"
        }
      },
      "dotnet": {
        "namespace": "Amazon.Cdk.QuickStarts"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/awslabs/aws-cdk"
  },
  "scripts": {
    "build": "cdk-build",
    "watch": "cdk-watch",
    "lint": "cdk-lint",
    "pkglint": "pkglint -f",
    "test": "cdk-test"
  },
  "keywords": [
    "aws",
    "cdk",
    "constructs",
    "quickstarts"
  ],
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com"
  },
  "license": "LicenseRef-LICENSE",
  "devDependencies": {
    "pkglint": "^0.7.3-beta",
    "cdk-build-tools": "^0.7.3-beta"
  },
  "dependencies": {
    "@aws-cdk/cloudformation": "^0.7.3-beta",
    "@aws-cdk/core": "^0.7.3-beta",
    "@aws-cdk/ec2": "^0.7.3-beta",
    "@aws-cdk/iam": "^0.7.3-beta",
    "@aws-cdk/rds": "^0.7.3-beta",
    "@aws-cdk/util": "^0.7.3-beta"
  },
  "bundledDependencies": [
    "@aws-cdk/util"
  ]
}
