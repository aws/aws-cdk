    "organization": true
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-cdk/cfnspec": "0.0.0",
    "colors": "^1.4.0",
    "diff": "^4.0.2",
    "fast-deep-equal": "^3.1.1",
    "string-width": "^4.2.0",
    "table": "^5.4.6"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/string-width": "^4.0.1",
    "@types/table": "^4.0.7",
    "cdk-build-tools": "0.0.0",
    "fast-check": "^1.23.0",
    "jest": "^24.9.0",
    "pkglint": "0.0.0",
    "ts-jest": "^25.2.0"
  },
  "repository": {
    "url": "https://github.com/aws/aws-cdk.git",
