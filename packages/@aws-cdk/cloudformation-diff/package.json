{
  "name": "@aws-cdk/cloudformation-diff",
  "version": "1.22.0",
  "description": "Utilities to diff CDK stacks against CloudFormation templates",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "cdk-build",
    "watch": "cdk-watch",
    "lint": "cdk-lint",
    "test": "cdk-test",
    "pkglint": "pkglint -f",
    "package": "cdk-package",
    "build+test+package": "npm run build+test && npm run package",
    "build+test": "npm run build && npm test"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "html",
      "text-summary"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 60,
        "lines": 60,
        "branches": 55
      }
    },
    "preset": "ts-jest",
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ]
  },
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com",
    "organization": true
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-cdk/cfnspec": "1.22.0",
    "@aws-cdk/cx-api": "1.22.0",
    "colors": "^1.4.0",
    "diff": "^4.0.2",
    "fast-deep-equal": "^3.1.1",
    "string-width": "^4.2.0",
    "table": "^5.4.6"
  },
  "devDependencies": {
    "@types/jest": "^25.1.0",
    "@types/string-width": "^4.0.1",
    "@types/table": "^4.0.7",
    "cdk-build-tools": "1.22.0",
    "fast-check": "^1.21.0",
    "jest": "^24.9.0",
    "pkglint": "1.22.0",
    "ts-jest": "^25.0.0"
  },
  "repository": {
    "url": "https://github.com/aws/aws-cdk.git",
    "type": "git",
    "directory": "packages/@aws-cdk/cloudformation-diff"
  },
  "keywords": [
    "aws",
    "cdk"
  ],
  "homepage": "https://github.com/aws/aws-cdk",
  "engines": {
    "node": ">= 10.3.0"
  },
  "stability": "experimental"
}
