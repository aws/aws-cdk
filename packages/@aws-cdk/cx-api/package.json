{
  "name": "@aws-cdk/cx-api",
  "version": "0.8.0",
  "description": "Cloud executable protocol",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "com.amazonaws.cdk.cxapi",
        "maven": {
          "groupId": "com.amazonaws.cdk",
          "artifactId": "cxapi"
        }
      },
      "dotnet": {
        "namespace": "Amazon.CDK.CXAPI"
      }
    }
  },
  "scripts": {
    "build": "cdk-build",
    "watch": "cdk-watch",
    "lint": "cdk-lint",
    "test": "cdk-test",
    "pkglint": "pkglint -f"
  },
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "cdk-build-tools": "^0.8.0",
    "pkglint": "^0.8.0"
  },
  "repository": {
    "url": "git://github.com/awslabs/aws-cdk",
    "type": "git"
  },
  "keywords": [
    "aws",
    "cdk"
  ]
}
