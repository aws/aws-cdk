{
  "name": "@aws-cdk/ec2",
  "version": "0.7.3-beta",
  "description": "CDK Constructs for AWS EC2",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "com.amazonaws.cdk.ec2",
        "maven": {
          "groupId": "com.amazonaws.cdk",
          "artifactId": "cdk-ec2"
        }
      },
      "dotnet": {
        "namespace": "Amazon.Cdk.Ec2"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/awslabs/aws-cdk"
  },
  "scripts": {
    "build": "cdk-build",
    "watch": "cdk-watch",
    "lint": "cdk-lint",
    "test": "cdk-test",
    "integ": "cdk-integ",
    "pkglint": "pkglint -f"
  },
  "cdk-build": {
    "cloudformation": "AWS::EC2"
  },
  "keywords": [
    "aws",
    "cdk",
    "constructs",
    "ec2"
  ],
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com"
  },
  "license": "LicenseRef-LICENSE",
  "devDependencies": {
    "@aws-cdk/assert": "^0.7.3-beta",
    "cfn2ts": "^0.7.3-beta",
    "pkglint": "^0.7.3-beta",
    "cdk-build-tools": "^0.7.3-beta",
    "cdk-integ-tools": "^0.7.3-beta"
  },
  "dependencies": {
    "@aws-cdk/autoscaling": "^0.7.3-beta",
    "@aws-cdk/core": "^0.7.3-beta",
    "@aws-cdk/elasticloadbalancing": "^0.7.3-beta",
    "@aws-cdk/iam": "^0.7.3-beta",
    "@aws-cdk/sns": "^0.7.3-beta",
    "@aws-cdk/util": "^0.7.3-beta"
  },
  "bundledDependencies": [
    "@aws-cdk/util"
  ]
}
