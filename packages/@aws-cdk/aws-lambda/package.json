{
  "name": "@aws-cdk/aws-lambda",
  "version": "1.0.0",
  "description": "CDK Constructs for AWS Lambda",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "software.amazon.awscdk.services.lambda",
        "maven": {
          "groupId": "software.amazon.awscdk",
          "artifactId": "lambda",
          "versionSuffix": ".DEVPREVIEW"
        }
      },
      "dotnet": {
        "namespace": "Amazon.CDK.AWS.Lambda",
        "packageId": "Amazon.CDK.AWS.Lambda",
        "signAssembly": true,
        "assemblyOriginatorKeyFile": "../../key.snk",
        "versionSuffix": "-devpreview"
      },
      "python": {
        "distName": "aws-cdk.aws-lambda",
        "module": "aws_cdk.aws_lambda"
      }
    },
    "metadata": {
      "awsdocs:title": "AWS Lambda"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/awslabs/aws-cdk.git",
    "directory": "packages/@aws-cdk/aws-lambda"
  },
  "scripts": {
    "build": "cdk-build",
    "watch": "cdk-watch",
    "lint": "cdk-lint",
    "test": "cdk-test",
    "integ": "cdk-integ",
    "pkglint": "pkglint -f",
    "package": "cdk-package",
    "awslint": "cdk-awslint",
    "cfn2ts": "cfn2ts",
    "build+test+package": "npm run build+test && npm run package",
    "build+test": "npm run build && npm test"
  },
  "cdk-build": {
    "cloudformation": "AWS::Lambda"
  },
  "nyc": {
    "statements": 75,
    "lines": 75
  },
  "keywords": [
    "aws",
    "cdk",
    "constructs",
    "lambda"
  ],
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com",
    "organization": true
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@aws-cdk/assert": "^1.0.0",
    "@types/aws-lambda": "^8.10.27",
    "@types/lodash": "^4.14.134",
    "@types/nock": "^10.0.3",
    "@types/sinon": "^7.0.13",
    "aws-sdk": "^2.438.0",
    "aws-sdk-mock": "^4.4.0",
    "cdk-build-tools": "^1.0.0",
    "cdk-integ-tools": "^1.0.0",
    "cfn2ts": "^1.0.0",
    "lodash": "^4.17.11",
    "nock": "^10.0.6",
    "pkglint": "^1.0.0",
    "sinon": "^7.3.2"
  },
  "dependencies": {
    "@aws-cdk/aws-cloudwatch": "^1.0.0",
    "@aws-cdk/aws-ec2": "^1.0.0",
    "@aws-cdk/aws-events": "^1.0.0",
    "@aws-cdk/aws-iam": "^1.0.0",
    "@aws-cdk/aws-logs": "^1.0.0",
    "@aws-cdk/aws-s3": "^1.0.0",
    "@aws-cdk/aws-s3-assets": "^1.0.0",
    "@aws-cdk/aws-sqs": "^1.0.0",
    "@aws-cdk/core": "^1.0.0",
    "@aws-cdk/cx-api": "^1.0.0"
  },
  "homepage": "https://github.com/awslabs/aws-cdk",
  "peerDependencies": {
    "@aws-cdk/aws-cloudwatch": "^1.0.0",
    "@aws-cdk/aws-ec2": "^1.0.0",
    "@aws-cdk/aws-events": "^1.0.0",
    "@aws-cdk/aws-iam": "^1.0.0",
    "@aws-cdk/aws-logs": "^1.0.0",
    "@aws-cdk/aws-s3": "^1.0.0",
    "@aws-cdk/aws-s3-assets": "^1.0.0",
    "@aws-cdk/aws-sqs": "^1.0.0",
    "@aws-cdk/core": "^1.0.0",
    "@aws-cdk/cx-api": "^1.0.0"
  },
  "engines": {
    "node": ">= 8.10.0"
  },
  "awslint": {
    "exclude": [
      "integ-return-type:@aws-cdk/aws-lambda.IEventSource.bind",
      "props-physical-name:@aws-cdk/aws-lambda.VersionProps",
      "props-physical-name:@aws-cdk/aws-lambda.EventSourceMappingProps"
    ]
  },
  "stability": "stable"
}
