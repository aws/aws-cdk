{
  "name": "@aws-cdk/aws-lambda",
  "version": "0.30.0",
  "description": "CDK Constructs for AWS Lambda",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "software.amazon.awscdk.services.lambda",
        "maven": {
          "groupId": "software.amazon.awscdk",
          "artifactId": "lambda"
        }
      },
      "dotnet": {
        "namespace": "Amazon.CDK.AWS.Lambda",
        "packageId": "Amazon.CDK.AWS.Lambda",
        "signAssembly": true,
        "assemblyOriginatorKeyFile": "../../key.snk"
      },
      "python": {
        "distName": "aws-cdk.aws-lambda",
        "module": "aws_cdk.aws_lambda"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/awslabs/aws-cdk.git",
    "directory": "packages/@aws-cdk/aws-lambda"
  },
  "scripts": {
    "build": "cdk-build",
    "watch": "cdk-watch",
    "lint": "cdk-lint",
    "test": "cdk-test",
    "integ": "cdk-integ",
    "pkglint": "pkglint -f",
    "package": "cdk-package",
    "awslint": "cdk-awslint",
    "cfn2ts": "cfn2ts"
  },
  "cdk-build": {
    "cloudformation": "AWS::Lambda"
  },
  "nyc": {
    "statements": 75,
    "lines": 75
  },
  "keywords": [
    "aws",
    "cdk",
    "constructs",
    "lambda"
  ],
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com",
    "organization": true
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@aws-cdk/assert": "^0.30.0",
    "@types/aws-lambda": "^8.10.24",
    "@types/nock": "^9.3.1",
    "@types/sinon": "^7.0.11",
    "aws-sdk": "^2.438.0",
    "aws-sdk-mock": "^4.4.0",
    "cdk-build-tools": "^0.30.0",
    "cdk-integ-tools": "^0.30.0",
    "cfn2ts": "^0.30.0",
    "nock": "^10.0.6",
    "pkglint": "^0.30.0",
    "sinon": "^7.3.1"
  },
  "dependencies": {
    "@aws-cdk/assets": "^0.30.0",
    "@aws-cdk/aws-cloudwatch": "^0.30.0",
    "@aws-cdk/aws-ec2": "^0.30.0",
    "@aws-cdk/aws-events": "^0.30.0",
    "@aws-cdk/aws-iam": "^0.30.0",
    "@aws-cdk/aws-logs": "^0.30.0",
    "@aws-cdk/aws-s3": "^0.30.0",
    "@aws-cdk/aws-s3-notifications": "^0.30.0",
    "@aws-cdk/aws-sqs": "^0.30.0",
    "@aws-cdk/aws-stepfunctions": "^0.30.0",
    "@aws-cdk/cdk": "^0.30.0",
    "@aws-cdk/cx-api": "^0.30.0"
  },
  "homepage": "https://github.com/awslabs/aws-cdk",
  "peerDependencies": {
    "@aws-cdk/assets": "^0.30.0",
    "@aws-cdk/aws-cloudwatch": "^0.30.0",
    "@aws-cdk/aws-ec2": "^0.30.0",
    "@aws-cdk/aws-events": "^0.30.0",
    "@aws-cdk/aws-iam": "^0.30.0",
    "@aws-cdk/aws-logs": "^0.30.0",
    "@aws-cdk/aws-s3": "^0.30.0",
    "@aws-cdk/aws-s3-notifications": "^0.30.0",
    "@aws-cdk/aws-sqs": "^0.30.0",
    "@aws-cdk/aws-stepfunctions": "^0.30.0",
    "@aws-cdk/cdk": "^0.30.0",
    "@aws-cdk/cx-api": "^0.30.0"
  },
  "engines": {
    "node": ">= 8.10.0"
  },
  "awslint": {
    "exclude": [
      "construct-base-is-private:@aws-cdk/aws-lambda.FunctionBase",
      "grant-result:@aws-cdk/aws-lambda.LayerVersion.grantUsage",
      "from-method:@aws-cdk/aws-lambda.Alias",
      "from-attributes:fromAliasAttributes",
      "from-method:@aws-cdk/aws-lambda.SingletonFunction",
      "from-method:@aws-cdk/aws-lambda.Version"
    ]
  }
}
