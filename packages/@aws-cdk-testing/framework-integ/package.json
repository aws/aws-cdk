    "@aws-cdk/lambda-layer-kubectl-v30": "^2.0.4",
    "@aws-cdk/lambda-layer-kubectl-v31": "^2.0.3",
    "@aws-cdk/lambda-layer-kubectl-v32": "^2.0.3",
    "aws-cdk-lib": "0.0.0",
    "cdk8s": "2.69.50",
    "cdk8s-plus-27": "2.9.5",
    "constructs": "^10.0.0"
  },
  "repository": {
    "url": "https://github.com/aws/aws-cdk.git",
    "type": "git",
    "directory": "packages/@aws-cdk-testing/framework-integ"
  },
  "keywords": [
    "aws",
    "cdk"
  ],
  "homepage": "https://github.com/aws/aws-cdk",
  "engines": {
    "node": ">= 14.15.0"
