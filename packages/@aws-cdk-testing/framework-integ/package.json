{
  "name": "@aws-cdk-testing/framework-integ",
  "description": "Integration tests for aws-cdk-lib",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "cdk-build",
    "watch": "cdk-watch",
    "lint": "cdk-lint",
    "pkglint": "pkglint -f",
    "test": "cdk-test",
    "integ": "integ-runner --language javascript",
    "package": "cdk-package",
    "build+test": "yarn build && yarn test",
    "build+extract": "yarn build",
    "build+test+package": "yarn build+test && yarn package",
    "build+test+extract": "yarn build+test"
  },
  "pkglint": {
    "exclude": [
      "package-info/repository",
      "dependencies/cdk-point-dependencies"
    ]
  },
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com",
    "organization": true
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@aws-cdk/cdk-build-tools": "0.0.0",
    "@aws-cdk/integ-runner": "2.184.1-alpha.0",
    "@aws-cdk/pkglint": "0.0.0",
    "@aws-sdk/client-acm": "3.632.0",
    "@aws-sdk/client-rds": "3.632.0",
    "@aws-sdk/client-s3": "3.632.0",
    "@aws-sdk/client-cognito-identity-provider": "3.632.0",
    "axios": "1.8.3",
    "delay": "5.0.0"
  },
  "dependencies": {
    "@aws-cdk/integ-tests-alpha": "0.0.0",
    "@aws-cdk/lambda-layer-kubectl-v24": "^2.0.242",
