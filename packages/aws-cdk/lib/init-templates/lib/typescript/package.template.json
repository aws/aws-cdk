{
    "name": "%name%",
    "version": "%cdk-version%",
    "description": "A CDK Construct Library",
    "private": true,
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "jsii": {
        "outdir": "dist",
        "names": {
            "java": "com.acme.cdk.%name.camelCased%",
            "dotnet": "Acme.Cdk.%name.PascalCased%"
        }
    },
    "scripts": {
        "prepare": "jsii && tslint -p .",
        "watch": "jsii -w",
        "lint": "tsc && tslint -p .",
        "test": "nodeunit test/test.*.js"
    },
    "devDependencies": {
        "@types/node": "^8.9.4",
        "jsii": "^0.4.0",
        "tslint": "^5.10.0",
        "typescript": "^2.8.3"
    }
}
