{
  "name": "aws-cdk",
  "description": "CDK Toolkit, the command line tool for CDK apps",
  "version": "0.7.2-beta",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "cdk": "bin/cdk"
  },
  "scripts": {
    "prepare": "/bin/bash generate.sh && tslint -p . && tsc && chmod +x bin/cdk && pkglint",
    "watch": "tsc -w",
    "lint": "tsc && tslint -p . --force",
    "pkglint": "pkglint -f",
    "test": "nodeunit test/test.*.js"
  },
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com"
  },
  "license": "LicenseRef-LICENSE",
  "devDependencies": {
    "@types/fs-extra": "^4.0.8",
    "@types/minimatch": "^3.0.3",
    "@types/request": "^2.47.1",
    "@types/uuid": "^3.4.3",
    "@types/yamljs": "^0.2.0",
    "@types/yargs": "^8.0.3",
    "pkglint": "^0.7.1"
  },
  "dependencies": {
    "@aws-cdk/cloudformation-diff": "^0.7.2-beta",
    "@aws-cdk/core": "^0.7.2-beta",
    "@aws-cdk/cx-api": "^0.7.2-beta",
    "@aws-cdk/resources": "^0.7.2-beta",
    "@aws-cdk/util": "^0.7.2-beta",
    "aws-sdk": "^2.259.1",
    "camelcase": "^5.0.0",
    "colors": "^1.2.1",
    "decamelize": "^2.0.0",
    "fs-extra": "^4.0.2",
    "json-diff": "^0.3.1",
    "minimatch": ">=3.0",
    "promptly": "^0.2.0",
    "request": "^2.83.0",
    "source-map-support": "^0.5.6",
    "yamljs": "^0.2.0",
    "yargs": "^9.0.1"
  },
  "repository": {
    "url": "git://github.com/awslabs/aws-cdk",
    "type": "git"
  },
  "keywords": [
    "aws",
    "cdk"
  ]
}
