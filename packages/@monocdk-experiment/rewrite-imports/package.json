{
  "name": "@monocdk-experiment/rewrite-imports",
  "version": "0.0.0",
  "description": "Rewrites typescript 'import' statements from @aws-cdk/xxx to monocdk-experiment",
  "bin": {
    "rewrite-imports": "bin/rewrite-imports"
  },
  "main": "lib/rewrite.js",
  "types": "lib/rewrite.d.ts",
  "scripts": {
    "build": "cdk-build",
    "watch": "cdk-watch",
    "test": "cdk-test",
    "pkglint": "pkglint -f",
    "package": "cdk-package",
    "build+test+package": "npm run build+test && npm run package",
    "build+test": "npm run build && npm test"
  },
  "keywords": [
    "aws",
    "cdk",
    "awscdk"
  ],
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com",
    "organization": true
  },
  "license": "Apache-2.0",
  "dependencies": {
    "glob": "^7.1.6"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "cdk-build-tools": "0.0.0",
    "pkglint": "0.0.0"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 75,
        "branches": 65
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/aws-cdk.git",
    "directory": "packages/@monocdk-experiment/rewrite-imports"
  },
  "homepage": "https://github.com/aws/aws-cdk",
  "stability": "experimental",
  "maturity": "developer-preview",
  "engines": {
    "node": ">= 10.12.0"
  }
}
