{
  "name": "aws-cdk-toolkit",
  "description": "CDK Toolkit, the command line tool for CDK apps",
  "version": "0.6.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "cdk": "bin/cdk"
  },
  "scripts": {
    "prepare": "tslint -p . && tsc && chmod +x bin/cdk && pkglint",
    "watch": "tsc -w",
    "lint": "tsc && tslint -p . --force",
    "pkglint": "pkglint -f"
  },
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "pkglint": "^0.6.0",
    "@types/fs-extra": "^4.0.8",
    "@types/request": "^2.0.6",
    "@types/yamljs": "^0.2.0",
    "@types/yargs": "^8.0.3",
    "@types/minimatch": "^3.0.3",
    "@types/uuid": "^3.4.3"
  },
  "dependencies": {
    "aws-cdk": "^0.6.0",
    "aws-cdk-cloudformation-diff": "^0.6.0",
    "aws-cdk-cx-api": "^0.6.0",
    "aws-cdk-resources": "^0.6.0",
    "aws-cdk-util": "^0.6.0",
    "aws-sdk": "^2.135.0",
    "colors": "^1.2.1",
    "fs-extra": "^4.0.2",
    "json-diff": "^0.3.1",
    "minimatch": ">=3.0",
    "promptly": "^0.2.0",
    "request": "^2.83.0",
    "source-map-support": "^0.5.6",
    "yamljs": "^0.2.0",
    "yargs": "^9.0.1"
  },
  "repository": {
    "url": "git://github.com/awslabs/aws-cdk",
    "type": "git"
  },
  "keywords": [
    "aws",
    "cdk"
  ]
}
